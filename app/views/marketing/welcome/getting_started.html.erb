<div class="getting-started marketing-welcome-getting-started">
  <header class="header">
    <%= render partial: "layouts/marketing/navbar" %>
  </header>

  <section class="hero-section">
    <div>
      <h3><%= t(".hero.title") %></h3>
      <small><%= t(".hero.subtitle") %></small>
    </div>
  </section>

  <section class="section let-us-handle-it">
    <div>
      <article>
        <h4><%= t(".let_us_handle_it.title")%></h4>
          <%= t(".let_us_handle_it.article") %>
      </article>

      <aside>
      </aside>
    </div>
  </section>

  <section class="enrollment-link">
    <div>
      <h4><%= t(".enrollment_link.title") %></h4>
        <%= t(".enrollment_link.article") %>
    </div>
  </section>

  <section class="guided-setup section">
    <div>
      <aside>

      </aside>

      <article>
        <h4><%= t(".guided_set_up.title")%></h4>
          <%= t(".guided_set_up.article") %>
      </article>
  </section>


  <section class="more-info">
    <div>
      <div>
        <h4><%= t(".more_info.title") %><h4>
      </div>
      <%= link_to t(".more_info.link"), how_it_works_path, class: "button orange" %>
    </div>
  </section>

  <%= render partial: "footer" %>

  <% if Rails.env.production? %>
    <%= render partial: "layouts/shared/reamaze" %>
  <% end %>
</div>
